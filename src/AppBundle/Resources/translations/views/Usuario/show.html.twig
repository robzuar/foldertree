<div class="row">
    <div class="col-md-12">
        <!-- begin panel -->
        <div class="panel panel-inverse">
            <div class="panel-heading">
                <div class="panel-heading-btn">
                </div>
                <h2 style="font-size: 14px" class="panel-title">{{ singular }}</h2>
            </div>
            <div class="panel-body">
                <form class="form-horizontal">
                    <div class="form-group">
                        <label class="col-md-3 control-label">Nombres</label>
                        <div class="col-md-9">
                            <a class="form-control">{% if entity.nombres is not null %}{{ entity.nombres }}{% endif %}  {% if entity.apellidos is not null %}{{ entity.apellidos }}{% endif %}</a>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">Correo</label>
                        <div class="col-md-9">
                            <a  class="form-control" disabled >{{ entity.email}}</a>
                        </div>
                    </div>
                    <div class="form-group">
                    <label class="col-md-3 control-label">Acceso a modulos</label>
                        <div class="col-md-9">
                            {% for accesos in entity.accesos  %}
                                <a  class="form-control" disabled >{{ accesos }}</a><br>
                            {% else %}
                                <a  class="form-control" disabled >Sin Accesos</a>
                            {% endfor %}
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>