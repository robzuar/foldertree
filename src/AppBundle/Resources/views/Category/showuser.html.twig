<div class="row">
    <div class="col-md-12">
        <!-- begin panel -->
        <div class="panel panel-inverse">
            <div class="panel-heading">
                <div class="panel-heading-btn">
                </div>
                <h2 style="font-size: 14px" class="panel-title">{{ singular }}</h2>
            </div>
            <div class="panel-body">
                <table>

                            {% for usuario in usuarios %}
                                <tr>{{ usuario.fullname }} / </tr>
                            {% endfor %}

                </table>
            </div>
        </div>
    </div>
</div>