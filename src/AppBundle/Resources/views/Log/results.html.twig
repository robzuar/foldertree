
<hr>
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-inverse">
            <div class="panel-heading">
                <div class="panel-heading-btn">
                    <a href="#" class="btn btn-medium btn-icon btn-circle btn-white btn-add"><i class="fa fa-2x fa-plus-circle"></i></a>
                </div>
                <h2 style="font-size: 14px" class="panel-title">{{ plural }}</h2>
            </div>
            <div class="panel-body">
                <table id="table-entities" class="table table-bordered table-responsive">
                    <thead>
                        <tr>
                            <th class="hide"></th>
                            <th>Accion - Nombre</th>
                            <th>Tipo - Nombre</th>
                            <th>Fecha</th>
                            <th>Usuario</th>
                        </tr>
                    </thead>
                    <tbody>
                    {% for entity in entities %}
                            <tr>
                                <td class="hide">{{ entity.id }}</td>
                                <td>{{ entity.accion }} {% if  entity.path != "" %}- {{ entity.path }}{% endif %}</td>
                                <td>{{ entity.entidad }} - {{ entity.nombre }}</td>
                                <td>{{ entity.dateCreado|date('d-m-Y') }}</td>
                                <td>{{ entity.usuarioCreate }}</td>

                            </tr>
                    {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
